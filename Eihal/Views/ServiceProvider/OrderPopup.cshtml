<script>

    function loadSteps() {
        var current_fs, next_fs, previous_fs; //fieldsets
        var opacity;
        var current = 1;
        var steps = $("fieldset").length;

        setProgressBar(current);

        $(".next").click(function () {
            var styleAttributeValue = $(".progress-bar").attr("style");
            // Check if the "style" attribute contains the style "width: 50%;"
            if (styleAttributeValue && styleAttributeValue.includes("width: 50%;")) {
                $("#main-class-modal").addClass("row justify-content-center");
            } else {

                $("#main-class-modal").removeClass("row justify-content-center");
            }
            current_fs = $(this).parent();
            next_fs = $(this).parent().next();

            //Add Class Active
            $("#progressbar li").eq($("fieldset").index(next_fs)).addClass("active");

            //show the next fieldset
            next_fs.show();
            //hide the current fieldset with style
            current_fs.animate({ opacity: 0 }, {
                step: function (now) {
                    // for making fielset appear animation
                    opacity = 1 - now;

                    current_fs.css({
                        'display': 'none',
                        'position': 'relative'
                    });
                    next_fs.css({ 'opacity': opacity });
                },
                duration: 500
            });
            setProgressBar(++current);
        });

        $(".previous").click(function () {
            var styleAttributeValue = $(".progress-bar").attr("style");
            if (styleAttributeValue && styleAttributeValue.includes("width: 50%;")) {
                $("#main-class-modal").addClass("row justify-content-center");
            } else {

                $("#main-class-modal").removeClass("row justify-content-center");
            }

            current_fs = $(this).parent();
            previous_fs = $(this).parent().prev();

            //Remove class active
            $("#progressbar li").eq($("fieldset").index(current_fs)).removeClass("active");

            //show the previous fieldset
            previous_fs.show();

            //hide the current fieldset with style
            current_fs.animate({ opacity: 0 }, {
                step: function (now) {
                    // for making fielset appear animation
                    opacity = 1 - now;

                    current_fs.css({
                        'display': 'none',
                        'position': 'relative'
                    });
                    previous_fs.css({ 'opacity': opacity });
                },
                duration: 500
            });
            setProgressBar(--current);
        });

        function setProgressBar(curStep) {
            var percent = parseFloat(100 / steps) * curStep;
            percent = percent.toFixed();
            $(".progress-bar")
                .css("width", percent + "%")
        }

        $(".submit").click(function () {
            return false;
        })

        // Event listener for the "Cancel" button
        $("#close-order").click(function () {
            // Reset the form to Step 1
            $("#patient-info").css("opacity", "");
            $("fieldset").hide();
            $("fieldset:first").show();
            $("#progressbar li").removeClass("active");
            $("#progressbar li:first").addClass("active");
            setProgressBar(1);
            current = 1;
        });
    }

    function getStates(countryId) {
        $.ajax({
            url: '@Url.Action("GetStatesDDL", "ServiceProvider")' + '?countryId=' + countryId,
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                // Clear existing options
                $('#states-ddl-wk').empty();
                // Populate the dropdown list
                var dropdown = $('#states-ddl-wk');
                // Add default option
                dropdown.append($('<option></option>').val('').text('Select a state'));
                $.each(data, function (index, item) {
                    dropdown.append($('<option></option>').val(item.id).text(item.titleEn));
                });
            }
        });
    }

